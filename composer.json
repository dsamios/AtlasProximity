{
	"name": "laravel/laravel",
	"type": "project",
	"description": "The Laravel Framework.",
	"keywords": [
		"framework",
		"laravel"
	],
	"license": "MIT",
	"require": {
		"php": "^7.1.3",
		"ext-curl": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"artesaos/seotools": "^0.12.1",
		"austinb/gameq": "~3.0",
		"barryvdh/laravel-ide-helper": "^2.5",
		"barryvdh/laravel-snappy": "^0.4.3",
		"binarytorch/larecipe": "^1.2",
		"doctrine/dbal": "^2.9",
		"fideloper/proxy": "^4.0",
		"ipunkt/laravel-analytics": "^1.5",
		"jeroennoten/laravel-adminlte": "^1.24",
		"khill/lavacharts": "^3.1",
		"laravel/framework": "5.7.*",
		"laravel/telescope": "^1.0",
		"laravel/tinker": "^1.0",
		"nopjmp/discord-webhooks": "^0.3.1",
		"predis/predis": "^1.1",
		"spatie/laravel-permission": "^2.29",
		"webpatser/laravel-uuid": "^3.0",
		"xpaw/php-source-query-class": "^1.0"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "^3.2",
		"beyondcode/laravel-dump-server": "^1.0",
		"deployer/recipes": "^6.2",
		"filp/whoops": "^2.0",
		"fzaninotto/faker": "^1.4",
		"laravel/homestead": "^8.0",
		"mockery/mockery": "^1.0",
		"nunomaduro/collision": "^2.0",
		"phpunit/phpunit": "^7.0"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/"
		},
		"classmap": [
			"database/seeds",
			"database/factories"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi"
		],
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php artisan key:generate --ansi"
		]
	}
}
